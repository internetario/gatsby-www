{
  "name": "gatsby-starter-typescript-plus",
  "description": "A starter kit for TypeScript-based Gatsby projects with sensible defaults.",
  "version": "1.0.0",
  "private": true,
  "author": "Resi Respati <resir014@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "npm run clean && gatsby build",
    "clean": "rimraf public .cache",
    "deploy": "gatsby build --prefix-paths",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "tslint 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.0",
    "@material-ui/icons": "^3.0.2",
    "@wapps/gatsby-plugin-material-ui": "^1.2.0",
    "babel-plugin-styled-components": "^1.7.1",
    "classnames": "^2.2.6",
    "gatsby": "^2.0.3",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-canonical-urls": "^2.0.5",
    "gatsby-plugin-google-analytics": "^2.0.7",
    "gatsby-plugin-manifest": "=2.0.4",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sass": "^2.0.4",
    "gatsby-plugin-sharp": "^2.0.5",
    "gatsby-plugin-sitemap": "^2.0.2",
    "gatsby-plugin-typescript": "^2.0.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^2.0.1",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-source-prismic": "^2.1.0",
    "gatsby-transformer-json": "^2.1.1",
    "gatsby-transformer-remark": "^2.1.3",
    "gatsby-transformer-sharp": "^2.1.1",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "modern-normalize": "^0.5.0",
    "node-sass": "^4.10.0",
    "normalize.css": "^8.0.0",
    "polished": "^2.0.3",
    "prism-themes": "^1.0.1",
    "prismic-reactjs": "^0.3.2",
    "prismjs": "^1.24.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "typescript": "^3.0.3",
    "unstated": "^2.1.1"
